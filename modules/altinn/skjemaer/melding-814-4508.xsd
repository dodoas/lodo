                <xs:element minOccurs="0" ref="BeregningAvInngaendeSaldoPaKontoForTilbakeholdtBeskattetInnte-grp-5045"/>
                <xs:element minOccurs="0" ref="BeregningAvSkattepliktigInntektForRederiselskap-grp-5050"/>
            </xs:sequence>
            <xs:attribute fixed="814" name="skjemanummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="4508" name="spesifikasjonsnummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="RF-1197" name="blankettnummer" type="xs:string" use="optional"/>
