    <xs:element name="GrunnlagForSarskiltFradrag-grp-2647">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>Grunnlag for særskilt fradrag</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
