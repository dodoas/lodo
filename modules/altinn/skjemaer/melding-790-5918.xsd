                <xs:element minOccurs="0" ref="RISKBeregningAksjerNOKUSEkstraordinartUtbytteDeltakeroppgave-datadef-27463">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>725 Andel av ekstraordinært utbytte utdelt i 2006</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her føres forskjellen mellom avsatt utbytte pr. 31.12.2005 og faktisk  utdelt utbytte i 2006.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="RISKBeregningAksjerNOKUSDeltakersAndelDeltakeroppgave-datadef-27464">
