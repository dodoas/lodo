                <xs:element minOccurs="0" ref="BeregningAvSkjermingsfradragOgSkattepliktigUtbytte-grp-6668"/>
                <xs:element minOccurs="0" ref="ReguleringAvAksjensInngangsverdi-grp-6669"/>
                <xs:element minOccurs="0" ref="OpplysningerOmRealisasjonMv-grp-2346"/>
                <xs:element minOccurs="0" ref="AjourholdAvInngangsverdiOgSkjermingsgrunnlag-grp-6670"/>
            </xs:sequence>
            <xs:attribute fixed="790" name="skjemanummer" type="xs:integer" use="required"/>
