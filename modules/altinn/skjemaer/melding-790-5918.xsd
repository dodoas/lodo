                <xs:element minOccurs="0" ref="Selskap-grp-2722"/>
                <xs:element minOccurs="0" ref="Eierforhold-grp-2347"/>
            </xs:sequence>
            <xs:attribute fixed="2339" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
