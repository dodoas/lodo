    <!--title='RF-1113: Søknad om utsatt frist for levering av selvangivelse for klienter' lang='NOB'-->
    <xs:element name="Skjema">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>RF-1113 Regnskapsførers-/revisors søknad om utsatt frist for levering  av selvangivelser på vegne av klienter 2006</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                    <p>Skjemaet skal sendes inn av regnskapsførere/revisorer som søker om  utsettelse for inntill 10 prosent av sine klienter. Ved  fristutsettelse må selvangivelsene leveres elektronisk.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>RF-1113 Regnskapsførars-/revisors søknad om utsett frist for levering  av sjølvmeldingar på vegne av klientane 2006</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HINT">
                    <p>Skjemaet skal brukast av rekneskapsførar/revisor som søkjer om  utsetjing for inntil 10 prosent av klientane. Gjes det utsetjing må  sjølvmeldingane leverast elektronisk</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="Soknad-grp-5616"/>
                <xs:element minOccurs="0" ref="skattyter-grp-5624"/>
            </xs:sequence>
            <xs:attribute fixed="1087" name="skjemanummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="6096" name="spesifikasjonsnummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="RF-1113" name="blankettnummer" type="xs:string" use="optional"/>
            <xs:attribute fixed="Søknad om utsatt frist for levering av selvangivelse for klienter" name="tittel" type="xs:string" use="optional"/>
            <xs:attribute fixed="5615" name="gruppeid" type="xs:positiveInteger" use="optional"/>
            <xs:attribute name="etatid" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="974761076">
                            <xs:annotation>
                                <xs:documentation>SKATTEDIREKTORATET</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:anyAttribute/>
        </xs:complexType>
    </xs:element>
