                <xs:element minOccurs="0" ref="PosterTilBeregningForDeltakerne-grp-3908"/>
                <xs:element minOccurs="0" ref="Inntredelsesbeskatning-grp-3919"/>
            </xs:sequence>
            <xs:attribute fixed="878" name="skjemanummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="2076" name="spesifikasjonsnummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="RF-1187" name="blankettnummer" type="xs:string" use="optional"/>
            <xs:attribute fixed="Selskapets oppgave over deltakers inntekt i ANS, KS og NOKUS mv." name="tittel" type="xs:string" use="optional"/>
            <xs:attribute fixed="1561" name="gruppeid" type="xs:positiveInteger" use="optional"/>
