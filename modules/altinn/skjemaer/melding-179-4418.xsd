        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="PersoninntektEnkeltmannsforetakFiskeJordbrukSkogbruk-datadef-6716">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>1.6.1/1.6.2/1.6.3 Beregnet positiv personinntekt fra  enkeltpersonsforetak herunder mannskapslott fra fiske og fangst</p>
                            </brreg:tekst>
