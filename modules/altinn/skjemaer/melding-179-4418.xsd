    <xs:element name="FritidsbaterLavereVerdi-grp-2454">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>4.2.2 Fritidsbåter, salgverdi under kr. 50.000</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
