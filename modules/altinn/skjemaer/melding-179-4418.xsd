        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="InnboOgAnnetLosore-grp-2453"/>
                <xs:element maxOccurs="20" minOccurs="0" ref="FritidsbaterLavereVerdi-grp-2454"/>
