    <xs:element name="AndreOpplysninger-grp-260">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>Andre opplysninger</p>
                </brreg:tekst>
