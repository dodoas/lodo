                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>3.2.4/3.2.5/3.2.6 Sum</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>3.2.4/3.2.5/3.2.6 Sum</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
