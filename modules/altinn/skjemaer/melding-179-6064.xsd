        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="25" minOccurs="0" ref="Innskudd-grp-3414"/>
                <xs:element minOccurs="0" ref="BankinnskuddPostgiroinnskudd-datadef-187">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>4.1.1 Sum</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
