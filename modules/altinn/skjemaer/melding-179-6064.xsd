            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                    <p>Privat innbu og annan lausøyre skal førast under post 4.2 anten det er  i Noreg eller i utlandet.</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="InnboOgAnnetLosore-grp-2453"/>
                <xs:element maxOccurs="20" minOccurs="0" ref="FritidsbaterLavereVerdi-grp-2454"/>
                <xs:element minOccurs="0" ref="InnboLosoreMVForFradrag-datadef-6817">
