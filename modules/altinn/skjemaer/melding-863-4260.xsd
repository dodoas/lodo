    <xs:element name="VIOpplysningerVedrorendeVaningshuset-grp-1884">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>VI.Anvendelse av tidligere framførte underskudd våningshus</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                    <p>Postene 201 - 209 skal bare anvendes dersom det kreves fradrag for  tidligere fremførte underskudd ved driftav våningshus.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>VI Anvending av tidligare framført underskott våningshus</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                    <p>Postane 201 - 209 skal berre brukast dersom det blir krevd frådrag for  tidlegare framførte underskott ved &lt;&lt;drift >> av våningshus.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HINT">
                    <p>gjell også utleigd</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="DriftsunderskuddAvVaningshus-grp-1893"/>
            </xs:sequence>
            <xs:attribute fixed="1884" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="DriftsunderskuddAvVaningshus-grp-1893">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>IV: Anvendelse av årets eller tidligere framførte underskudd  våningshus</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>Bruk av årets eller tidlegare framførte underskot våningshus</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="ResultatJordbrukPelsdyrInklusiveANS-datadef-19778">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>201 Årsresultat jordbruk - hentes fra post 0402B i RF-1175 (evt 0204B  i RF-1167) inkludert evt. andel av overskudd/underskudd fra  ANS/DA/jordbruk</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Tallet hentes fra næringsoppgaven post 402E. Tallet viser netto  resultat i næringen. Et eventuelt overskudd/underskudd fra ansvarlig  selskap i jordbruk/pelsdyrnæring medtas her.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>201 Årsresultat jordbruk - skal hentast frå post 0402B i RF-1175 (ev  0204B i RF-1167) inkludert ev. del av overskott/underskott frå  ANS/DA-jordbruk</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Talet skal hentast frå næringsoppgåva, post 402. Talet viser netto  resultat i næringa. Eit eventuelt underskott/overskott frå ansvarleg  selskap i jordbruk/pelsdyrnæring skal takast med her.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="FradragFesteavgiftMvVaningshus-datadef-16391">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>202 Fradrag for festeavgifter, årlige utbetalinger for fallrettigheter  og gevinst fra tomtesalg</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Dersom beløpet i post 201 inneholder inntekter fra a)gevinst fra  tomtesalg, enten ført direkte til inntekt eller over gevinst- og  tapskonto  b)festeavgifter eller  c)årlige utbetalinger for  fallrettigheter, skal
                                    disse trekkes fra i denne posten.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>202 Frådrag for festeavgifter, årlege utbetaligar for fallrettar og  vinst frå tomtesal</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Dersom beløpet i post 201 inneheld inntekter frå a)gevinst fra  tomtesal, anten ført direkte til inntekt eller over gevinst- og  tapskonto  b)festeavgifter eller  c)årlege utbetalinger for  fallrettar, skal desse
                                    trekkast frå i denne posten.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="SkogbruksinntektPositiv-datadef-15555">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>203 Årets positive skogbruksinntekt etter gjennomsnittsberegningen  (hentes fra RF-1175 post0402E evt fra RF-1167 post 0402E,evt. fra  RF-1221 post 102g).Redusert med eventuelle festeavgifter mv, jf. post  202</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her tillegges årets positive inntekt fra skogbruk, som hentes fra post  0402E i næringsoppgaven. Også andel av overskudd/underskudd fra evt.  deltakerlignet skogsameie (fra post 102g i RF-1221) tas med her.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>203 Årets positive skogbruksinntekt etter gjennomsnittsutrekning (skal  hentast frå RF-1175 post0402E ev fra RF-1167 post 0402E,ev. frå  RF-1221 post 102g). Redusert med eventuelle festeavgifter o.a, jf.  post 202</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Her føre ein  positiv skattepliktig inntekt i 2005 frå skogbruk, som  skal hentast fra post 0402E i næringsoppgåva. Også andel av  overskott/underskott fra evt. deltakarligna skogsameie (frå post 102g  i RF-1221)
                                    skal takast med her.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="OverskuddPelsdyrnaring-datadef-22235">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>204 Årets positive inntekt fra pelsdyrnæring - hentes fra RF-1175 post  0402B (evt. RF-1167 post 0204B)</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her tillegges årets overskudd fra pelsdyrnæring.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>204 Årets positive inntekt fra pelsdyrnæring - skal hentast frå  RF-1175 post 0402B (evt. RF-1167 post 0204B)</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Her skal ein føre overskott frå pelsdyrnæring.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="SykepengerJordbrukMv-datadef-15571">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>205 Sykepenger utbetalt på grunnlag av personinntekt fra jordbruk,  skogbruk og pelsdyrnæring</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her tillegges eventuelle utbetalte sykepenger på grunnlag av  personinntekt i de ovennevnte næringene.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>205 Sjukepengar utbetalte på grunnlag av personinntekt frå jordbruk,  skogbruk og pelsdyrnæring</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Her fører ein eventuelle utbetalte sjukepengar på grunnlag av  personinntekt i dei næringane som er nemnde ovanfor.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="ResultatJordbrukSkogbrukPelsdyrEkslUnderskuddVaningshus-datadef-19781">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>206 Sum - hvis negativt beløp, behøver postene nedenfor ikke fylles  ut.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Null eller negativt beløp her betyr at det ikke er grunnlag for  fradrag for tidligere fremført underskudd fra drift våningshuset.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>Hvis negativt beløp, behøver postene nedenfor ikke fylles ut. Da føres  tallet fra post 206 med positivt fortegn direkte til postene 231 og  0402D i næringsoppgaven</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>206 Sum - er beløpet negativt, skal ikkje postane nedanfor  fyllast  ut. Før då talet i post 206 med positivt forteikn direkte til postane  231 og post 0402D i RF-1175 (ev. post 0402D i RF-1167)</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Null eller negativt beløp betur at det ikkje er grunnlag for frådrag  for tidlegare fremført underskott frå drift av våningshuset.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HINT">
                                <p>er beløpet negativt, skal ikkje postane nedanfor fyllast ut. Før då  talet i post 206 med positivt forteikn direkte til postane 231 og  0402D i næringsoppgaven</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="JordbruksfradragPositivt-datadef-22478">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>207 Jordbruksfradrag - hentes fra post 437</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her fratrekkes årets jordbruksfradrag som hentes fra post 437 på side  4.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>207 Jordbruksfrådrag</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Her trekker ein får jordbruksfrådraget for 2005, som ein hentar frå  post 437 på side 4.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="ResultatJordSkogPelsInklUnderskVaningshusEkslJordbrFradrag-datadef-19783">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>208 Positivt beløp føres til 209c, dersom det i post 230f er beløp til  fremføring</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Dersom beløpet i post 208 blir positivt, kan tidligere fremførte  underskudd fradras så langt beløpet rekker. Beregningen skjer i post  209 og beløpet føres til post 209c.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>208 Positivt beløp skal førast til 209c, dersom det i post 230f er  beløp til framføring</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Dersom beløpet i post 208 blir positivt, kan tidlegare framførte  underskott trekkjast frå så langt beløpet rekker. Utrekninga skjer i  post 209, og beløpet skal førast til post 209c.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute fixed="1893" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ResultatJordbrukPelsdyrInklusiveANS-datadef-19778">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="DEF">
                    <p>Årsresultat fra jordbruk og eventuell pelsdyrnæring tillagt eventuelt andel av overskudd i ANS fra disse næringene.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:info brreg:type="plassering">57</brreg:info>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
