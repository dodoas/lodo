                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>111 Rekneskapsmessig verdi på aksjar og andre verdipapir</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Her fører ein rekneskapsmessig verdi på aksjar og andre verdipapir pr.  31.12.2004 og 31.12.2005.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
