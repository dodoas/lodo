        </xs:complexType>
    </xs:element>
    <xs:element name="MalemetodeUtslippEndringBeskrivelse-datadef-24216">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="DEF">
                    <p>Beskrivelse av endringer i målemetoder for kvotepliktige CO2-utslipp</p>
