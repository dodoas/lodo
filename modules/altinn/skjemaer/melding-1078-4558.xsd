    <xs:element name="UtslippFraRaffineri-grp-5649">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>2.1 Rapportering av CO2-utslipp fra raffinerier</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                    <p>Utslipp av CO2 fra forbrenning av energivarer knyttet til  raffineriproduksjon skal rapporteres i del 1.  </p>
                    <p>Prosessrelaterte CO2-utslipp fra raffinerier kan omfatte utslipp fra  produksjon av hydrogen, regenerering av katalysator, kalsinering og  andre katalytiske prosesser.</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="999" minOccurs="0" ref="UtslippFraRaffinerier-grp-5518"/>
                <xs:element minOccurs="0" ref="UtslippRaffineriTorrRoykgassVolum-datadef-24173">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
