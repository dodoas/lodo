    <xs:element name="UtslippKnyttetTilBearbeidingAvJernOgStal-grp-5512">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>2.2. Rapportering av CO2-utslipp fra produksjon og bearbeiding av jern  og stål, herunder røsting og sintring.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                    <p>Utslipp av CO2 fra forbrenning av energivarer knyttet til produksjon  og bearbeiding av jern og stål skal rapporteres i del 1.  </p>
                    <p/>
                    <p>De årlige prosessrelaterte utslippene av CO2 fra produksjon og  bearbeiding av jern og stål skal beregnes ut fra karbonbalansen etter  følgende ligning:  </p>
                    <p/>
                    <p>Utslipp CO2 = Sum(tonn C fra karbonmaterialer tilført prosessen ÷ tonn  C bundet i produkter, biprodukter og avfall, ÷ tonn C eksportert  eller solgt som gass til annen virksomhet) * 3,667</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="999" minOccurs="0" ref="TilfortKarbon-grp-5526"/>
                <xs:element minOccurs="0" ref="UtslippJernStalTilfortKarbonTonn-datadef-24181">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Sum tilført karbon:</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element maxOccurs="999" minOccurs="0" ref="BundetKarbonIProdukter-grp-5527"/>
                <xs:element minOccurs="0" ref="UtslippJernStalBundetKarbonTonn-datadef-24184">
