    <xs:element name="AndreForhold-grp-5369">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>6.03 Andre forhold</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>6.03 Andre forhold</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="RegnskapsprinsippEndringSisteTreAr-datadef-23777">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>6.03.01 Er det gjennomført endringer av regnskapsprinsipp i løpet av  de siste tre år?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Dette omfatter ikke endringer av regnskapsprinsipp som følge av  overgang til IFRS (International Financial Reporting Standards).</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>6.03.01 Er det gjort endringar i rekneskapsprinsippa dei siste tre  åra?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Det gjeld ikkje endringar av rekneskapsprinsipp som følgje av overgang  til IFRS (International Financial Reporting Standards).</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="LanebetingelserBruddDispensasjon-datadef-23778">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>6.03.02 Foreligger det brudd på eller dispensasjon fra lånebetingelser  (covenants)?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>6.03.02 Er det brot på eller dispensasjon frå lånevilkåra (covenants)?</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="Inntektsvekst25SisteAr50TreSisteAr-datadef-23779">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>6.03.03 Overstiger veksten i samlede inntekter 25 % av siste års  inntekter eller mer enn 50 % av de siste tre års inntekter?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Med samlede inntekter forstås foretakets totale inntekter. Med tre  siste år menes de tre årene forut for perioden det rapporteres for.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>6.03.03 Er veksten i dei samla inntektene større enn 25 % av  inntektene det siste året eller meir enn 50 % av inntektene dei siste  tre åra?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Med samla inntekter meiner vi dei totale inntektene til føretaket. Med  tre siste år meiner vi dei tre åra før perioden det blir rapportert  for.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="SkattesakerVesentligeTapteSisteToAr-datadef-23780">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>6.03.04 Har foretaket tapt vesentlige skattesaker i løpet av de siste  to år?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Med vesentlig menes om utsteder har opplyst om forholdet i noter eller  annen tilleggsinformasjon i års-/delårsregnskapet.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>6.03.04 Har føretaket tapt vesentlege skattesaker dei siste to åra?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Med vesentleg meiner vi om utskrivaren har opplyst om forholdet i  merknader eller annan tilleggsinformasjon i års-/delårsrekneskapen.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
