                <xs:element ref="RapportKvartalAr-datadef-18965">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>1.10 Rapporteringsperiode</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Velg riktig rapporteringsperiode. Det kan velges mellom 1. kvartal, 2.  kvartal, 3. kvartal, 4. kvartal / foreløpig årsregnskap eller  årsregnskap.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="FEIL">
                                <p>1.10 Periode/kvartal må velges</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>1.10 Rapporteringsperiode</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                                <p>Vel rett rapporteringsperiode. Du kan velje mellom 1. kvartal,  2. kvartal, 3. kvartal, 4. kvartal / førebels årsrekneskap eller  årsrekneskap.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
