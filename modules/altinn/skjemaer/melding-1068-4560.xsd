            </xs:sequence>
            <xs:attribute fixed="1068" name="skjemanummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="4560" name="spesifikasjonsnummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="KRT-1003" name="blankettnummer" type="xs:string" use="optional"/>
            <xs:attribute fixed="Rapportering fra utstederforetak ved Oslo Børs" name="tittel" type="xs:string" use="optional"/>
