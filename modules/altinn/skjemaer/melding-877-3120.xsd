                <xs:element minOccurs="0" ref="OpplysningerOmErvervEllerRealisasjonAvAksjerAndeler-grp-2069"/>
            </xs:sequence>
            <xs:attribute fixed="877" name="skjemanummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="3120" name="spesifikasjonsnummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="RF-1234" name="blankettnummer" type="xs:string" use="optional"/>
            <xs:attribute fixed="Selskapets oppgave over deltakeres formue og inntekt i NOKUS mv" name="tittel" type="xs:string" use="optional"/>
            <xs:attribute fixed="2059" name="gruppeid" type="xs:positiveInteger" use="optional"/>
