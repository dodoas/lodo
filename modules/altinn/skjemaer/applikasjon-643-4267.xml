                <xforms:repeat id="repeat_OppsummeringAlleKraftverk-grp-5083" nodeset="/Skjema/OppsummeringAlleKraftverk-grp-385/OppsummeringAlleKraftverk-grp-5083">
                    <xforms:group ref="/Skjema/OppsummeringAlleKraftverk-grp-385/OppsummeringAlleKraftverk-grp-5083">
                        <brreg:sensitivitet type="Sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Oppsummering alle kraftverk</p>
                        </xforms:caption>
                        <xforms:input ref="/Skjema/OppsummeringAlleKraftverk-grp-385/OppsummeringAlleKraftverk-grp-5083/AnleggLopenummer-datadef-19639">
                            <brreg:sensitivitet type="Sensitiv"/>
