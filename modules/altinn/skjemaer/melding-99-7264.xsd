              <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                  <p>223 Fradrag for korreksjonsinntekt fra tidligere år</p>
                </brreg:tekst>
              </xs:documentation>
              <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
