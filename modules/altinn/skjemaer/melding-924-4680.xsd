                                <p>Feltet krev to desimalar</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="AksjerBeholdningFjoraretEndring-datadef-24416">
                    <xs:annotation>
