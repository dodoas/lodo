    <xs:element name="DetteAr-grp-882">
      <xs:annotation>
        <xs:documentation>
          <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
            <p>2007</p>
          </brreg:tekst>
        </xs:documentation>
