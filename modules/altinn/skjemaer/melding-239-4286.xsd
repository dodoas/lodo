                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>e) Korreksjon reindrift; Avviklings- og omstillingsfond, (Uttak ÷  Innskudd)</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>e) Korreksjon reindrift; avviklings- og omstillingsfond, (Uttak ÷  Innskudd)</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="InntektAlminneligANSAndel-datadef-17012">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>f) Sum alm. inntekt</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
