                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>640 Innskutt selskapskapital / ek-konto som overførast 2007</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
