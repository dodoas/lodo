                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="ErververAndelKSFodselsOrganisasjonsnummerSelskapsoppgave-datadef-26630">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Erververs fødsels-/organisasjonsnummer</p>
                            </brreg:tekst>
                        </xs:documentation>
