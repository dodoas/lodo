                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                  <p>44 Skilnader/endring i skilnader</p>
                </brreg:tekst>
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
