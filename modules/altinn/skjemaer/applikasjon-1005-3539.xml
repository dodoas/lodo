<?xml version="1.0" encoding="iso-8859-1"?>
<brreg:skjema xmlns:brreg="http://www.brreg.no/or" xmlns:xforms="http://www.w3.org/2002/01/xforms" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <brreg:tittel brreg:lang="NOB">RA-0532-MK: Undersøkelse om utenriksøkonomi - måneds/kvartalsrapportering</brreg:tittel>
    <brreg:hode>
        <xforms:model>
            <!--INSTANCE HOLDER: Instansen kan plasseres her!-->
            <xforms:schema xlink:href="melding-1005-3540.xsd"/>
            <!--SUBMITINFO HOLDER: SubmitInfo kan plasseres her!-->
            <xforms:bind readOnly="true" ref="/Skjema/GenerellInformasjon-grp-4879/OppgaveInnsendelseFrist-datadef-16296"/>
            <xforms:bind readOnly="true" ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetNavn-datadef-1"/>
            <xforms:bind readOnly="true" ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetOrganisasjonsnummer-datadef-18"/>
            <xforms:bind readOnly="true" ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetAdresse-datadef-15"/>
            <xforms:bind readOnly="true" ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetPostnummer-datadef-6673"/>
            <xforms:bind readOnly="true" ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetPoststed-datadef-6674"/>
            <xforms:bind ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/KontaktpersonNavn-datadef-2" required="true"/>
            <xforms:bind ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/KontaktpersonTelefonnummer-datadef-3" required="true"/>
        </xforms:model>
    </brreg:hode>
    <brreg:kropp>
        <xforms:group ref="/Skjema">
            <xforms:caption brreg:lang="NOB">
                <p>RA-0532-MK. Rapportering om utenriksøkonomi.  Måneds/kvartalsrapportering.</p>
            </xforms:caption>
            <xforms:group ref="/Skjema/GenerellInformasjon-grp-4879">
                <brreg:sensitivitet type="Ikke sensitiv"/>
                <xforms:caption brreg:lang="NOB">
                    <p>Generell informasjon</p>
                </xforms:caption>
                <xforms:hint brreg:lang="NOB">
                    <p>Innsending av fil som er laget fra Excel-skjemaet gjøres ved å klikke  på arkfanen vedlegg til venstre.  </p>
                    <p>Rapporten skal gjelde den norske delen av den juridiske personen, dvs.  den juridiske personen eksk. eventuelle filialer i utlandet.  </p>
                    <p>Rapporten skal inneholde tall for hittil i perioden. Kontroller om  tallene avser riktig periode.</p>
                </xforms:hint>
                <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/OppgaveInnsendelseFrist-datadef-16296">
                    <brreg:preutfyll/>
                    <brreg:sensitivitet type="Ikke sensitiv"/>
                    <xforms:caption brreg:lang="NOB">
                        <p>Innsendelsesfrist 30 dager etter periodens utløp</p>
                    </xforms:caption>
                </xforms:input>
                <xforms:selectOne ref="/Skjema/GenerellInformasjon-grp-4879/OppgavePeriode-datadef-21812">
                    <xforms:choices>
                        <xforms:item>
                            <xforms:value>K</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Kvartalsrapportering</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>M</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Månedsrapportering</p>
                            </xforms:caption>
                        </xforms:item>
                    </xforms:choices>
                    <brreg:sensitivitet type="Ikke sensitiv"/>
                    <xforms:caption brreg:lang="NOB">
                        <p>Velg rapporteringsperiode:</p>
                    </xforms:caption>
                </xforms:selectOne>
                <xforms:selectOne ref="/Skjema/GenerellInformasjon-grp-4879/OppgaveKvartalsrapportering-datadef-21813">
                    <xforms:choices>
                        <xforms:item>
                            <xforms:value>1</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>1. kvartal</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>2</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>2. kvartal</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>3</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>3. kvartal</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>4</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>4. kvartal</p>
                            </xforms:caption>
                        </xforms:item>
                    </xforms:choices>
                    <brreg:sensitivitet type="Ikke sensitiv"/>
                    <xforms:caption brreg:lang="NOB">
                        <p>Oppgi rapporteringsperiode for de vedlagte kvartalsdata:</p>
                    </xforms:caption>
                </xforms:selectOne>
                <xforms:selectOne ref="/Skjema/GenerellInformasjon-grp-4879/OppgaveManedsrapportering-datadef-21814">
                    <xforms:choices>
                        <xforms:item>
                            <xforms:value>1</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Januar</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>2</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Februar</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>3</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Mars</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>4</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>April</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>5</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Mai</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>6</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Juni</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>7</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Juli</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>8</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>August</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>9</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>September</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>10</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Oktober</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>11</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>November</p>
                            </xforms:caption>
                        </xforms:item>
                        <xforms:item>
                            <xforms:value>12</xforms:value>
                            <xforms:caption brreg:lang="NOB">
                                <p>Desember</p>
                            </xforms:caption>
                        </xforms:item>
                    </xforms:choices>
                    <brreg:sensitivitet type="Ikke sensitiv"/>
                    <xforms:caption brreg:lang="NOB">
                        <p>Oppgi rapporteringsperiode for de vedlagte månedsdata:</p>
                    </xforms:caption>
                </xforms:selectOne>
                <xforms:group ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880">
                    <brreg:sensitivitet type="Ikke sensitiv"/>
                    <xforms:caption brreg:lang="NOB">
                        <p>Rapporteringsenhet</p>
                    </xforms:caption>
                    <xforms:hint brreg:lang="NOB">
                        <p>Rett eventuelle feil i boksene nedenfor.</p>
                    </xforms:hint>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetNavn-datadef-1">
                        <brreg:preutfyll/>
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Navn</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetOrganisasjonsnummer-datadef-18">
                        <brreg:preutfyll/>
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <brreg:kontroll type="Modulus 11 kontroll med vektene 3, 2, 7, 6, 5, 4, 3, 2"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Organisasjonsnummer</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetAdresse-datadef-15">
                        <brreg:preutfyll/>
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Postadresse</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetPostnummer-datadef-6673">
                        <brreg:preutfyll/>
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Postnummer</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Rapporteringsenhet-grp-4880/EnhetPoststed-datadef-6674">
                        <brreg:preutfyll/>
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Poststed</p>
                        </xforms:caption>
                    </xforms:input>
                </xforms:group>
                <xforms:group ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881">
                    <brreg:sensitivitet type="Ikke sensitiv"/>
                    <xforms:caption brreg:lang="NOB">
                        <p>Kontaktperson</p>
                    </xforms:caption>
                    <xforms:hint brreg:lang="NOB">
                        <p>Oppgi alltid navn på kontaktperson som SSB kan kontakte.</p>
                    </xforms:hint>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/KontaktpersonNavn-datadef-2">
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Navn</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/KontaktpersonTelefonnummer-datadef-3">
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Telefonnummer</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/KontaktpersonTelefaxnummer-datadef-324">
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Telefaks</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/KontaktpersonEPost-datadef-2876">
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Kontaktpersonens epostadresse</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-4879/Kontaktperson-grp-4881/EnhetEPost-datadef-5963">
                        <brreg:sensitivitet type="Ikke sensitiv"/>
                        <xforms:caption brreg:lang="NOB">
                            <p>Rapporteringsenhetens E-post</p>
                        </xforms:caption>
                        <xforms:hint brreg:lang="NOB">
                            <p>Husk vedlegg før innsending. Gå til arkfanen vedlegg oppe til venstre  i bildet. Legg til vedlegget. Send data.</p>
                        </xforms:hint>
                    </xforms:input>
                </xforms:group>
            </xforms:group>
        </xforms:group>
    </brreg:kropp>
</brreg:skjema>
