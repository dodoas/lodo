    <xs:element name="KorrigertOverkurs-grp-5586">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>6.2 Korrigert overkurs</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                    <p>Posten skal brukes i de tilfeller hvor aksjonær har fått tilbakebetalt  eller har innbetalt overkurs uten at det er hensyntatt i  anskaffelsesverdien.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                    <p>per aksje</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="AksjerKorrigertOverkurs-datadef-24405">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Opplysninger fra skattemyndighetene</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="AksjerKorrigertOverkursEndring-datadef-24417">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Korreksjoner fra aksjonær</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Eventuelt manglende endringer i overkurs per aksje skal føres her.  Dersom det er blitt utbetalt overkurs, skal inngangsverdien  reduseres. Dette betyr at negativt fortegn skal benyttes i feltet.  </p>
                                <p/>
                                <p>Dersom det er blitt innbetalt overkurs, skal dette føre til økning av  inngangsverdien. Størrelsen legges inn uten fortegn, og håndteres som  et positivt tall.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute fixed="5586" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>
