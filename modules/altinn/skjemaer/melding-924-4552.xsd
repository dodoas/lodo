    <xs:element name="KorrigeringAvInngangsverdiMedTilbakebetaltInnbetaltAksjekapit-grp-5588">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>6.4 Korrigering av inngangsverdi med tilbakebetalt  / innbetalt  aksjekapital</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                    <p>Posten skal brukes i de tilfeller aksjonær har innbetalt  aksjekapital/eller fått tilbakebetalt aksjekapital som ikke er  hensyntatt i skattyters anskaffelsesverdi. Eksempel på dette er  nyemisjoner med økning av pålydende i
                        2003 som ikke er korrekt  behandlet av Aksjonærregisteret.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                    <p>per aksje</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="AksjerInngangsverdiKorrigeringAksjekapitalendring-datadef-24408">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Opplysninger fra skattemyndighetene</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="AksjerInngangsverdiKorrigeringAksjekapitalendringEndring-datadef-24421">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Korreksjon fra aksjonær</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Ved tilbakebetaling av innbetalt aksjekapital, skal dette føre til  reduksjon av inngangsverdi (negativt fortegn)  </p>
                                <p/>
                                <p>Ved økning av aksjekapital ved innbetaling, skal dette føre til økning  av inngangsverdi. Størrelsen legges inn uten fortegn, og håndteres  som et positivt tall.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute fixed="5588" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
