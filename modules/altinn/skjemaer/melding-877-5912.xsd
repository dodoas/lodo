        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="AksjerNOKUSKostprisAnskaffelseAretsInngangSelskapsoppgave-datadef-27499">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>900 Kostpris og anskaffelseskostnader pr. 01.01.2006</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her oppgis historisk kostpris for aksjene, evt. kapitalinnskudd i  selskapet ved stiftelse eller emisjon, for de aksjer deltaker eide  pr. 1.1.2006.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="AksjerNOKUSKostprisRealiserteSelskapsoppgave-datadef-27501">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>910 Kostpris på realiserte andeler</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Her oppgis kostpris på de aksjer som er realisert i løpet av 2006.  Beløpet skal være overensstemmende med beløpet i post 810.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>jf. post 810</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
