    <!--title='RF-1117: Klage på likningen for næringsdrivende' lang='NOB'-->
    <xs:element name="Skjema">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>RF-1117   Klage på likning for næringsdrivende</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                    <p>Skjema benyttes av næringsdrivende skattytere. NB! Inneholder klagen  sensetiv informajon (f. eks. om sykdom) kan den ikke leveres  elektrioniske. Klagen sendes da med brev til likningskontoret.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>RF-1117 Klage på likninga for næringsdrivande</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HJELP">
                    <p>Skjema gjeld næringsdrivande skattytare. NB! Inneheld klaga sensetive  opplysningar (t.d om sjukdom) kan du ikkje levere klaga elektronisk.  Ho må sendast i brev til likningskontoret.</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="Skattyterinfor-grp-5801"/>
                <xs:element minOccurs="0" ref="klage-grp-5805"/>
            </xs:sequence>
            <xs:attribute fixed="1243" name="skjemanummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="5120" name="spesifikasjonsnummer" type="xs:integer" use="required"/>
            <xs:attribute fixed="RF-1117" name="blankettnummer" type="xs:string" use="optional"/>
            <xs:attribute fixed="Klage på likningen for næringsdrivende" name="tittel" type="xs:string" use="optional"/>
            <xs:attribute fixed="5800" name="gruppeid" type="xs:positiveInteger" use="optional"/>
            <xs:attribute name="etatid" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="974761076">
                            <xs:annotation>
                                <xs:documentation>SKATTEDIREKTORATET</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:anyAttribute/>
        </xs:complexType>
    </xs:element>
