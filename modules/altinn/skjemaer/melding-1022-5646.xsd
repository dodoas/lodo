                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="Konsernspiss-grp-5613"/>
            </xs:sequence>
            <xs:attribute fixed="5085" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
