    <xs:element name="KostnaderTilForskningsOgUtviklingsprosjekter-grp-3683">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>200 Kostnader til forsknings- og utviklingsprosjekter</p>
