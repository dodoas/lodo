            <xs:attribute fixed="RF-1053" name="blankettnummer" type="xs:string" use="optional"/>
            <xs:attribute fixed="Fradrag i skatt for kostnader til forsknings- og utviklingsprosjekter" name="tittel" type="xs:string" use="optional"/>
            <xs:attribute fixed="2227" name="gruppeid" type="xs:positiveInteger" use="optional"/>
            <xs:attribute name="etatid" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="974761076">
                            <xs:annotation>
                                <xs:documentation>SKATTEDIREKTORATET</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:anyAttribute/>
        </xs:complexType>
    </xs:element>
