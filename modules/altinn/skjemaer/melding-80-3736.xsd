    <xs:element name="FordelingMellomEktefeller-grp-303">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>5 Fordeling mellom ektefeller når disse driver felles bedrift</p>
                </brreg:tekst>
