    <xs:element name="SarskiltePosterForDeltakere-grp-6695">
        <xs:annotation/>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="OverskuddFinansForFremforing-datadef-17635">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>425A Finansinntekt/-underskudd</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Post 425.  </p>
                                <p>Sum skattepliktig finansinntekt/fradragsberettiget finansunderskudd  overføres fra RF-1197 post 770. Overskudd og underskudd fordeles  etter eierandel og overføres til RF-1246 Deltakerens oppgave over  egen formue
                                    og inntekt i norskkontrollert utenlandsk selskap i  lavskatteland mv., henholdsvis post 502 for finansoverskudd og post  301 for finansunderskudd.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>fra RF-1197 post 770</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="FinansinntektLigningSkattelov810-datadef-22111">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>425C</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>Overføres RF-1234/1246 post 301/502(NOK)</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="GevinstTapskontoRederiInntektsfort-datadef-13678">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>427A Inntektsføring fra gevinst- og tapskonto</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Post 427.  </p>
                                <p>Beløpet i post 427C overføres fra RF-1197 post 785. Andel av beløpet  skal overføres til RF-1246 post 508 for deltaker som på  inntredelsestidspunktet oppfylte vilkårene for inntektsføring fra  gevinst og tapskonto
                                    i NOKUS-selskapet.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>fra RF-1197 post 785</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="InntektsforingGevinstTapskontoLigningSkattelov810-datadef-22113">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>427C</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>Overføres RF-1234/1246 post 508(NOK)</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="InntektInntreden-datadef-12761">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>428A Inntektsføring ved inntreden</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Post 428.  </p>
                                <p>Inntekten i post 428C overføres fra RF-1197 post 790. Beløpet fordeles  etter eierandel og overføres RF-1246 post 510 for deltaker som har  trådt inn i rederibeskatningsordningen i inntektsåret.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>fra RF-1197 post 790</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="InntektInntredenLigningSkattelov810-datadef-22115">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>428C</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>Overføres RF-1234/1246 post 510(NOK)</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="InntektUtgiftSktl238NOKUSMvLigningSkattelov810Valuta-datadef-22755">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>429A Inntekter/gevinster og utgifter/tap som omfattes av sktl. §2-38</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Post 429.  </p>
                                <p>Etter sktl. § 8-15 tredje ledd femte og sjette punktum skal konto for  beskattet inntekt i rederibeskattet aksjeselskap reguleres med  finansinntekter og  -tap som er unntatt beskatning eller  fradragsføring etter
                                    sktl. § 2-38. Et rederibeskattet aksjeselskap  som er deltaker i NOKUS, skal derfor også medta sin andel av det  underliggende selskaps inntekter/gevinster og utgifter/tap som  omfattes av skatteloven § 2-38 ved
                                    regulering av konto for beskattet  inntekt.  </p>
                                <p>Slikt unntak gjelder for utbytte fra børsnoterte selskaper,  gevinst/tap ved realisasjon av slike aksjer, samt gevinst/tap ved  realisasjon av finansielle instrumenter som har slike aksjer som  underliggende objekt.
                                    Vilkåret for unntak etter § 2-38 er at  selskapet er hjemmehørende i EØS, eller i land utenfor EØS som ikke  er definert som lavskatteland. I land utenfor EØS som ikke er  lavskatteland, er vilkåret at eierandelen
                                    de siste 2 år ikke har vært  under 10 %. For utbytte er vilkåret ikke de siste to år, men en  to-årsperiode. Etter overgangsregel A må man i perioden fra 06.10.04  og frem til det aktuelle tidspunktet ikke på noe
                                    tidspunkt ha eiet  mindre enn 10 %, dog er utbytte likevel unntatt dersom man ikke eier  under 10% i en 2-årsperiode.  </p>
                                <p>Gevinst ved overføring av aksjer til deltakerens nærstående skal ikke  medtas når overføringen skjer 6. oktober 2004 eller senere, jf. sktl.  § 8-15 tredje ledd syvende til niende punktum.  </p>
                                <p>Ved beregning av aksjetap eller aksjegevinst skal aksjens  inngangsverdi ikke reguleres for RISK-beløp eller korreksjonsbeløp  etter sktl. § 10-32 fjerde og femte ledd.  </p>
                                <p>Beløpet i post 429C fordeles etter eierandel og overføres RF-1246 post  512.</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="InntektUtgiftSkatteloven238NOKUSMvLigningSkattelov810-datadef-22347">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>429C</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>Overføres RF-1234/1246 post 512(NOK)</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" ref="TonnasjeskattAndel-datadef-5876">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>430 Tonnasjeskatt RF-1197 post 520</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HJELP">
                                <p>Post 430.  </p>
                                <p>Beløpet overføres fra RF-1197 post 520. Beløpet fordeles etter  eierandel og overføres RF-1246 post 514.</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                                <p>Overføres RF-1234/1246 post 514(NOK)</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute fixed="6695" name="gruppeid" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>
