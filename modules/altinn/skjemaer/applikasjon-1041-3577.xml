                        <xforms:caption brreg:lang="NOB">
                            <p>Telefaks</p>
                        </xforms:caption>
                    </xforms:input>
                    <xforms:input ref="/Skjema/GenerellInformasjon-grp-5161/Kontaktperson-grp-5164/KontaktpersonEPost-datadef-2876">
                        <brreg:sensitivitet type="Ikke sensitiv"/>
