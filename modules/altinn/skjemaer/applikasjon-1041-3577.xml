                                    <p>Mai</p>
                                </xforms:caption>
                            </xforms:item>
                            <xforms:item>
                                <xforms:value>6</xforms:value>
                                <xforms:caption brreg:lang="NOB">
                                    <p>Juni</p>
                                </xforms:caption>
                            </xforms:item>
                            <xforms:item>
                                <xforms:value>7</xforms:value>
                                <xforms:caption brreg:lang="NOB">
                                    <p>Juli</p>
                                </xforms:caption>
                            </xforms:item>
                            <xforms:item>
                                <xforms:value>8</xforms:value>
                                <xforms:caption brreg:lang="NOB">
                                    <p>August</p>
                                </xforms:caption>
                            </xforms:item>
                            <xforms:item>
                                <xforms:value>9</xforms:value>
                                <xforms:caption brreg:lang="NOB">
                                    <p>September</p>
                                </xforms:caption>
