                        </xforms:alert>
                        <xforms:alert brreg:lang="NOB">
                            <p>Feltet krever positivt heltall med fire siffer</p>
                        </xforms:alert>
                        <xforms:alert brreg:lang="NON">
                            <p>Feltet krev positivt heiltal med fire siffer</p>
                        </xforms:alert>
