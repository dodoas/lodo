                            <xforms:caption brreg:lang="NOB">
                                <p>Endringsoppgave</p>
                            </xforms:caption>
                            <xforms:caption brreg:lang="NON">
                                <p>Endringsoppgåve</p>
                            </xforms:caption>
                        </xforms:item>
