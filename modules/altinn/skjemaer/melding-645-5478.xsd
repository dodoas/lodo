                <xs:element minOccurs="0" ref="Arsproduksjon-grp-2518"/>
                <xs:element minOccurs="0" ref="SumBruttoSalgsinntekt-grp-2519"/>
                <xs:element minOccurs="0" ref="InntektsforingAvGevinstVedRealisasjonAvDriftsmidlerSomBenyttes-grp-2520"/>
