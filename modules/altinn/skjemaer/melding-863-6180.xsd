    <xs:element name="UnderskuddFraTidligereAr-grp-2032">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>Underskudd fra tidligere år</p>
                </brreg:tekst>
            </xs:documentation>
