        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="EgenkapitalANSOpptjentErvervIAretDeltakeroppgave-datadef-26398">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>605 Ervervsbeløp i løpet av 2006</p>
                            </brreg:tekst>
                        </xs:documentation>
