                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="TilleggIAlminneligInntekt-grp-6393"/>
            </xs:sequence>
