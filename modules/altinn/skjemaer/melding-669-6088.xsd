    <xs:element name="Skjema">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>RF-1037 Terminoppgave for arbeidsgiveravgift og forskuddstrekk 2007</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                    <p>Du kan ikke bruke Altinn til å rette en innsendt oppgave. Har du  allerede sendt inn en oppgave med feil, må du kontakte  skatteoppkreveren.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>RF-1037 Terminoppgåve for arbeidsgivaravgift og forskottstrekk 2007</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HINT">
                    <p>Gjeld innmeldinga endringar i tidlegare innmeldte terminar  (endringsoppgåve), kan du ikkje nytte Altinn. Har du allereie sendt  inn en oppgave med feil, må du kontakte skatteoppkrevjaren.</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="Innledning-grp-986"/>
                <xs:element minOccurs="0" ref="Arbeidsgiveravgift-grp-5698"/>
                <xs:element minOccurs="0" ref="Arbeidsgiveravgift-grp-6719"/>
                <xs:element minOccurs="0" ref="Forskuddstrekk-grp-6717"/>
