            <xs:sequence>
                <xs:element ref="OppgaveSjofolk-datadef-24335">
                    <xs:annotation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                                <p>Gjelder oppgaven for sjøfolk med inntekt som gir rett til særskilt  fradrag for sjøfolk?</p>
                            </brreg:tekst>
                        </xs:documentation>
                        <xs:documentation>
                            <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                                <p>Gjeld oppgåva for sjøfolk med inntekt som gir rett til særskilt  frådrag for sjøfolk?</p>
                            </brreg:tekst>
                        </xs:documentation>
                    </xs:annotation>
