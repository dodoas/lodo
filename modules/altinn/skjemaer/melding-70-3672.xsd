    <xs:element name="BeregningAvMaksimaltTapsfradrag-grp-4166">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>Beregning av maksimalt tapsfradrag</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                    <p>Skal kun fylles ut hvis skatteyter benytter de oppregulerte  inngangsverdiene godkjent takst eller regnskapsmessig egenkapital og  beløpet i post 208 er negativt (tap)</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>Utrekning av maksimalt frådrag for tap</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HINT">
                    <p>Skal kun fylles ut hvis skattytar nyttar dei oppregulerte  inngangsverdiane godkjend takst eller rekneskapsmessig eigenkapital  og beløpet i post 208 er negativt (tap)</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="SkattemessigFormuesverdiOgHistoriskKostpris-grp-4167"/>
                <xs:element minOccurs="0" ref="BeregningAvMaksimaltTapsfradrag-grp-4168"/>
            </xs:sequence>
