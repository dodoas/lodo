    <xs:element name="FondsemisjonSplittSkattefriFusjonFisjonSammenslaingDelingAv-grp-3994">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>24 Fondsemisjon, splitt, skattefri fusjon/fisjon og  sammenslåing/deling av aksjeklasse</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>Fondsemisjon, splitt, skattefri fusjon, skattefri fisjon og  sammenslåing/deling av aksjeklasse</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="9999" minOccurs="0" ref="AntallAksjerITilgangIfmOmfordeling-grp-3999"/>
            </xs:sequence>
            <xs:attribute fixed="3994" name="gruppeid" type="xs:positiveInteger" use="required"/>
