    <xs:element name="TOENBeregnetPersoninntektOgGodtgjorelse-grp-2361">
        <xs:annotation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="LEDE">
                    <p>2.1 Beregnet personinntekt og godtgjørelse for arbeid knyttet til  virksomhet i utlandet</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NOB" brreg:teksttype="HINT">
                    <p>Inkl. i post nr.</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="LEDE">
                    <p>2.1 Utrekna personinntekt og godtgjering for arbeid knytt til verksemd  i utlandet</p>
                </brreg:tekst>
            </xs:documentation>
            <xs:documentation>
                <brreg:tekst brreg:lang="NON" brreg:teksttype="HINT">
                    <p>Inkl. i post nr.</p>
                </brreg:tekst>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="BelopINOK-grp-4241"/>
            </xs:sequence>
