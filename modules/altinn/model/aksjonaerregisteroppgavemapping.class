            	return 0;
        	}
        	else
	        {
            	$item = $this->Account[$a];
            	$this->ORID[$item] = $v;
	            return 1;
    	    }
       	}
    }

    function setHuman($h, $v)
    {
        $this->ORID[$this->Human[$h]] = $v;
    }

