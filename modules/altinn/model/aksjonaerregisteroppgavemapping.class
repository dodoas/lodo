        return $this->ORID;
    }

    function getHuman($a)
    {
        $tmpstr = $this->Human[$this->Account['D'.$a]];
        $tmp = explode('-', $tmpstr);
        return $tmp['0'];
    }
    
    function getHumanArray()
    {
        return $this->Human;
    }
    function getAccount()
